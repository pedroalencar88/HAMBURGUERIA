  object QVALIDACPF: TFDQuery
    Connection = DM_.FDCONEXAO
    SQL.Strings = (
      '--SELECT REPLACE(REPLACE(cpf,'#39'-'#39','#39#39'),'#39'.'#39','#39#39') AS CPF FROM CLIENTE'
